Subject: [PATCH] add handlerlist finally


diff --git a/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockBreakDropItemsEvent.java b/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockBreakDropItemsEvent.java
index 40226ada..7431f38b 100644
--- a/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockBreakDropItemsEvent.java
+++ b/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockBreakDropItemsEvent.java
@@ -17,6 +17,8 @@ public class BlockBreakDropItemsEvent extends BlockEvent implements Cancellable
         return handlers;
     }
 
+    public static HandlerList getHandlerList() { return handlers; }
+
     private final Player player;
     private boolean cancel;
     private List<ItemStack> drops;
diff --git a/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockPreBreakEvent.java b/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockPreBreakEvent.java
index f9d602b1..2c16bb4d 100644
--- a/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockPreBreakEvent.java
+++ b/src/main/java/net/gravitas/bandiadpaper/events/blocks/BlockPreBreakEvent.java
@@ -16,6 +16,7 @@ public class BlockPreBreakEvent extends BlockEvent implements Cancellable {
     public HandlerList getHandlers() {
         return handlers;
     }
+    public static HandlerList getHandlerList() { return handlers; }
 
     private final Player player;
     private boolean cancel;
